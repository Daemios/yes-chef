    "@vitejs/plugin-vue": "^4.5.2",
    "@vitest/coverage-c8": "^0.33.0",
    "@vitest/ui": "^0.33.0",
    "@vue/test-utils": "^2.4.6",
    "eslint": "^8.57.0",
    "eslint-plugin-vue": "^9.24.1",
    "happy-dom": "^13.5.0",
